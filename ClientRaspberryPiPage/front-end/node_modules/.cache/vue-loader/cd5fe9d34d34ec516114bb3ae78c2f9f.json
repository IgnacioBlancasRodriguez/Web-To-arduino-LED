Root":"","sourcesContent":["<template>\r\n  <button v-on:click=\"changeState\"><b>Turn {{ btnState }}</b></button>\r\n</template>\r\n<script>\r\n//import { sendClientType, sendState } from \"./scripts/sokcetsComs.js\";\r\nvar io = require(\"socket.io-client\");\r\nvar socket = io(\"http://localhost:3001\", {\r\n  transports: [\"websocket\"],\r\n  upgrade: false,\r\n  reconnect: false\r\n});\r\n\r\nexport default {\r\n  name: \"NavBar\",\r\n  data() {\r\n    return {\r\n      btnState: \"On\",\r\n      arduino: \"\",\r\n    };\r\n  },\r\n  mounted: () => {\r\n    let data = {\r\n        \"clientType\": \"client\",\r\n    };\r\n    socket.emit(\"client-type\", data);\r\n  },\r\n  methods: {\r\n    changeState() {\r\n      socket.emit(\"get-arduinos\", {\r\n        \"request\": \"get-arduinos\"\r\n      });\r\n      socket.on(\"get-arduinos-response\", (data) => {\r\n        console.log(data);\r\n        this.arduino = `${data[`arduinos_${ 0 }`]}`;\r\n        console.log(this.arduino);\r\n        socket.emit(\"change-state\", {\r\n          \"btnState\": `${ this.btnState }`,\r\n          \"target\": `${this.arduino}`\r\n          });\r\n          socket.on(\"change-state-response\", (data) => {\r\n            console.log(`Response to change-state request: ${ data }`);\r\n            this.btnState = data[\"newBtnState\"];\r\n          });\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style scoped>\r\nbutton {\r\n  font-family: Dosis,sans-serif;\r\n  font-size: 1.2em;\r\n  padding-top: 1em;\r\n  padding-bottom: 1em;\r\n  width: 15%;\r\n  background-color: #333333;\r\n  border-radius: 10px;\r\n  border-style: none;\r\n  color: whitesmoke;\r\n  transition: 0.3s;\r\n}\r\nbutton:hover {\r\n  background-color: #576574;\r\n}\r\n</style>"]}]}